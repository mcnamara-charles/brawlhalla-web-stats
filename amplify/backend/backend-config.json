{
  "storage": {
    "brawlhalladata": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    }
  },
  "function": {
    "getTableData": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda",
      "dependsOn": [
        {
          "category": "storage",
          "resourceName": "brawlhalladata",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  },
  "api": {
    "WorkflowsAPI": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "function",
          "resourceName": "getTableData",
          "attributes": [
            "Name",
            "Arn"
          ]
        }
      ]
    }
  }
}